@page
@model IndexModel
@{
	ViewData["Title"] = "Home page";
}

<h3>
	<a class="btn btn-link" asp-page="/Account/New">New</a>
</h3>
<table class="table">
	<thead>
		<tr>
			<td>#</td>
			<td>Name</td>
			<td>Claims</td>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model.UserList)
		{
		<tr>
			<td>@item.Id</td>
			<td>@item.UserName</td>
			<td>@string.Join("; ", item.Claimes)</td>
		</tr>
		}
	</tbody>
</table>